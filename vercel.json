{
    "rewrites": [
      {
        "source": "/studio(.*)",
        "destination": "/studio/$1"
      }
    ],
    "headers": [
      {
        "source": "/studio(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
        ]
      }
    ]
  }
  